# Uncomment the next line to define a global platform for your project
platform :ios, '12.0'

target 'RxSwift_playground' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!
  pod "RxSwift"
  pod "RxCocoa"

  # Pods for RxSwift_playground

  target 'RxSwift_playgroundTests' do
    inherit! :search_paths
    # Pods for testing
    pod "RxBlocking"
    pod "RxTest"
  end

  target 'RxSwift_playgroundUITests' do
    # Pods for testing
    pod "RxBlocking"
    pod "RxTest"
  end

end
